{% extends 'base.html.twig' %}


{% block body %}
    <h1>Modifier le Projet</h1>

    {{ form_start(form) }}
    {{ form_row(form.name) }}
    {{ form_row(form.description) }}
    {{ form_row(form.startDate) }}
    {{ form_row(form.endTime) }}
    {{ form_row(form.users) }}
    <button type="submit" class="btn btn-success">Modifier</button>
    {{ form_end(form) }}

{% endblock %}
